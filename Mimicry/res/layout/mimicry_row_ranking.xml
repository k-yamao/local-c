<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/LinearLayout_h1" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:orientation="horizontal"
	android:weightSum="1" android:background="@drawable/bgcolorranking">
	<LinearLayout android:id="@+id/linearLayout_row"
		android:orientation="horizontal" android:layout_height="wrap_content"
		android:layout_margin="1dp" android:layout_width="fill_parent"
		android:background="@drawable/white">

		<TextView android:id="@+id/ranking" style="@style/mimicryrow1"
			android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="10dp" android:layout_marginLeft="10dp">
		</TextView>
		<ImageView android:id="@+id/ranking_image" android:contentDescription="@string/image"
			android:layout_width="80dp" android:layout_height="80dp"
			android:layout_margin="10dp">
		</ImageView>

		<LinearLayout android:id="@+id/linearLayout_v1"
			android:layout_width="0dp" android:layout_height="120dp"
			android:layout_margin="5dp" android:layout_weight="1"
			android:orientation="vertical">

            <TextView android:id="@+id/nicname_text" style="@style/mimicryrow6"
                android:layout_width="wrap_content" android:layout_height="12dp"
                android:text="@string/nicname_text">
            </TextView>

			<TextView android:id="@+id/nicname" style="@style/mimicryrow1"
				android:layout_width="wrap_content" android:layout_height="25dp">
			</TextView>

			<TextView android:id="@+id/title_text" style="@style/mimicryrow6"
                android:layout_width="wrap_content" android:layout_height="12dp"
                android:text="@string/title_text">
            </TextView>

			<TextView android:id="@+id/title" style="@style/mimicryrow2"
				android:layout_width="wrap_content" android:layout_height="25dp">
			</TextView>
			<TextView android:id="@+id/contribution_date_txet" style="@style/mimicryrow6"
				android:layout_width="wrap_content" android:layout_height="15dp"
				android:text="@string/contribution_date_text">
			</TextView>

			<TextView android:id="@+id/contribution_date" style="@style/mimicryrow5"
				android:layout_width="wrap_content" android:layout_height="20dp">
			</TextView>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>